{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "baseUrl": ".",
    "paths": {
      "@internals/api": ["./src/api/index.ts"],
      "@internals/assets/*": ["./src/assets/*"],
      "@internals/components": ["./src/components/index.ts"],
      "@internals/constants": ["./src/constants/index.ts"],
      "@internals/helpers": ["./src/helpers/index.ts"],
      "@internals/hooks/*": ["./src/hooks/*"],
      "@internals/i18n": ["./src/i18n/index.ts"],
      "@internals/i18n/*": ["./src/i18n/*"],
      "@internals/mocks": ["./src/mocks/index.ts"],
      "@internals/navigators": ["./src/navigators/index.ts"],
      "@internals/models": ["./src/models/index.ts"],
      "@internals/screens": ["./src/screens/index.ts"],
      "@internals/store": ["./src/store/index.ts"],
      "@internals/styles": ["./src/styles/index.ts"],
      "@internals/theme": ["./src/theme/index.ts"],
      "@internals/types": ["./src/types/index.ts"],
      "@internals/storybook": ["./storybook"],
      "@internals/errors/*": ["./src/errors/*"],
      "@internals/contexts/*": ["./src/contexts/*"],
      "@internals/services/*": ["./src/services/*"],
      "@internals/models/view_model/*": ["./src/models/view_model/*"],
      "@internals/dto/*": ["./src/dto/*"],
      "@internals/models/*": ["./src/models/*"]
    }
  },
  "exclude": ["storybook", "node_modules", "vendor"]
}
